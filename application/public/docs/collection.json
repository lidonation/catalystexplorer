              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/v1/funds?page=1&per_page=24",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "v1",
                "funds"
              ],
              "query": [
                {
                  "key": "page",
                  "value": "1",
                  "description": "The page number for pagination"
                },
                {
                  "key": "per_page",
                  "value": "24",
                  "description": "Number of funds per page (max 60)"
                }
              ]
            },
            "description": "Retrieve a paginated list of Project Catalyst funding rounds with filtering, sorting, and relationship includes."
          }
        },
        {
          "name": "Get Fund Details",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Accept",
                "value": "application/json",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/v1/funds/:id?include=proposals,campaigns",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "v1",
                "funds",
                ":id"
              ],
              "query": [
                {
                  "key": "include",
                  "value": "proposals,campaigns",
                  "description": "Comma-separated list of relationships to include"
                }
              ],
              "variable": [
                {
                  "key": "id",
                  "value": "12",
                  "description": "The fund ID"
                }
              ]
            },
            "description": "Retrieve detailed information about a specific fund including optional related data."
          }
        },
        {
          "name": "Get Fund Proposals",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Accept",
                "value": "application/json",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/v1/funds/:id/proposals?page=1&per_page=24",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "v1",
                "funds",
                ":id",
                "proposals"
              ],
              "query": [
                {
                  "key": "page",
                  "value": "1",
                  "description": "The page number for pagination"
                },
                {
                  "key": "per_page",
                  "value": "24",
                  "description": "Number of proposals per page (max 60)"
                }
              ],
              "variable": [
                {
                  "key": "id",
                  "value": "12",
                  "description": "The fund ID"
                }
              ]
            },
            "description": "Get paginated proposals for a specific fund."
          }
        }
      ],
      "description": "APIs for managing and retrieving Project Catalyst funding rounds and campaigns"
    }
  ]
